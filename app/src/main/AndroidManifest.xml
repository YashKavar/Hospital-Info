<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hospitalinfo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HospitalInfo">
        <activity
            android:name=".AdminSettings"
            android:exported="true"
            android:label="Admin Settings"
            android:parentActivityName=".UserDashbord"/>
        <activity
            android:name=".AdminContactUs"
            android:exported="true"
            android:label="Contact Us"
            android:parentActivityName=".UserDashbord"/>
        <activity
            android:name=".AdminProfile"
            android:exported="true"
            android:label="Admin Profile"
            android:parentActivityName=".UserDashbord"/>
        <activity
            android:name=".Signup"
            android:exported="true"
            android:label="Admin Signup"
            android:parentActivityName=".Login" />
        <activity
            android:name=".Login"
            android:exported="true"
            android:label="Admin Login"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UserDashbord"
            android:exported="true"
            android:label="Admin Dashboard" />
        <activity
            android:name=".Settings"
            android:exported="true"
            android:label="Settings"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ContactUs"
            android:exported="true"
            android:label="Contact Us"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>